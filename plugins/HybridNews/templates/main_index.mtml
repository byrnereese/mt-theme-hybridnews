<html xmlns="http://www.w3.org/1999/xhtml" dir="ltr" lang="en-US" xml:lang="en-US">
<head profile="http://gmpg.org/xfn/11">
    <title><$mt:BlogName$><mt:if tag="BlogDescription" ne=""> | <$mt:BlogDescription encode_html="1"$></mt:if></title>
    <meta name="robots" content="index,follow" />
    <meta name="description" content="<$mt:BlogDescription encode_html="1"$>" />
<mt:ignore>
    <meta name="copyright" content="Copyright (c) February 2009" />
    <meta name="revised" content="Monday, February 2nd, 2009, 2:32 pm" />
    <meta name="template" content="Hybrid 0.5" />
</mt:ignore>
    <link rel="EditURI" type="application/rsd+xml" title="RSD" href="<$MTLink template="rsd"$>" />
    <$mt:include module="HTML Head"$>
    <link rel="stylesheet" href="<$mt:StaticWebPath$>plugins/HybridNews/css/front-page.css?ver=0.1" type="text/css" media="screen" />
</head>
<body class="ltr not-logged-in home front-page page page-7 page-template page-template-front-page page-author-admin mac firefox firefox-3 primary-active secondary-active subsidiary-active">
  <div id="body-container">
    <$mt:include module="Banner Header"$>
    <div id="container">
      <div id="content"> <!-- Deprecated. Use the .content or .hfeed class for styling instead of #content. -->
      <div class="hfeed content">

        <mt:IfSlideshowEnabled>
        <$mt:include module="Homepage Slideshow"$>
        </mt:IfSlideshowEnabled>

<mt:setvarblock name="excerptcount"><$mt:ExcerptCount$></mt:setvarblock>
        <div id="excerpts">
<mt:Entries unique="1" lastn="$excerptcount">
          <div class="hentry post post-<$mt:EntryID$> <mt:if name="__even__">even<mt:else>odd</mt:if> category-<mt:EntryCategory><$mt:CategoryBasename$></mt:EntryCategory> <MTEntryIfTagged><MTEntryTags glue=' '>tag-<$MTTagName regex_replace="/ /g","-"$></MTEntryTags></MTEntryIfTagged>">
<mt:if tag="EntryFeaturedImage">
  <mt:setvarblock name="aid"><mt:EntryFeaturedImage></mt:setvarblock>
  <mt:setvarblock name="thumbnail"><mt:IfNonEmpty tag="EntryFeaturedImage"><mt:EntryFeaturedImageAsset id="$aid"><img src="<mt:AssetThumbnailURL width="150" square="1"$>" alt="<$mt:AssetLabel encode_html="1"$>" class="thumbnail" /></mt:EntryFeaturedImageAsset></mt:IfNonEmpty></mt:setvarblock>
<mt:else>
  <mt:setvarblock name="thumbnail"><mt:EntryAssets lastn="1"><img src="<mt:AssetThumbnailURL width="150" square="1"$>" alt="<$mt:AssetLabel encode_html="1"$>" class="thumbnail" /></mt:EntryAssets></mt:setvarblock>
</mt:if>
<mt:if name="thumbnail"><div class="featured-image"><a href="<$mt:EntryPermalink$>"><$mt:var name="thumbnail"$></a></div></mt:if>
            <h2 class="post-title entry-title"><a href="<$mt:EntryPermalink$>" title="<$mt:EntryTitle encode_html="1"$>" rel="bookmark"><$mt:EntryTitle encode_html="1"$></a></h2>
            <p class="byline"><span class="text">By</span> <span class="author vcard"><a class="url fn n" href="<$mt:EntryAuthorURL$>" title="<$mt:EntryAuthorDisplayName$>"><$mt:EntryAuthorDisplayName$></a></span> <span class="text">on</span> <abbr class="published" title="<$MTEntryDate format_name="iso8601"$>"><$MTEntryDate format="%m %d, %y"$></abbr></p>
            <div class="entry-summary entry">
              <p><$mt:EntryExcerpt words="22"$></p>
            </div>
            <p class="entry-meta post-meta-data">
              <MTIfNonEmpty tag="EntryCategory"><span class="categories"><span class="text">Posted in</span> <MTEntryCategories glue=', '><a href="<$MTCategoryArchiveLink$>" rel="category"><$MTCategoryLabel$></a></MTEntryCategories></span></MTIfNonEmpty>
              <MTEntryIfTagged><span class="tags"> <MTIfNonEmpty tag="EntryCategory"><span class="separator">|</span></MTIfNonEmpty> <span class="text">Tagged</span> <MTEntryTags glue=', '><a href="<$mt:CGIPath$><$mt:SearchScript$>?IncludeBlogs=<$mt:BlogID$>&tag=<$mt:TagName regex_replace="/\s/g","+" url_encode="1"$>&limit=20" rel="tag"><$MTTagName$></a></MTEntryTags></span></MTEntryIfTagged>
              <MTIfCommentsActive><span class="separator">|</span> <a href="<$MTEntryPermalink$>#comments" class="comments-link" title="Comments on <$mt:EntryTitle encode_html="1"$>"><$MTEntryCommentCount singular="1 Response" plural="# Responses" none="No Responses"$></a></MTIfCommentsActive>
            </p>
          </div>
</mt:Entries>
        </div><!-- //end excerpts -->

        <div id="headlines">
<mt:var name="count" value="0">
<mt:TopLevelCategories>
<mt:setvar name="count" op="add" value="1">
<mt:setvarblock name="odd"><mt:var name="count" op="mod" value="2"></mt:setvarblock>

          <div class="section <mt:if name="odd">odd<mt:else>even</mt:if>">
            <h3 class="section-title"><a href="<$mt:CategoryArchiveLink$>" title="<$mt:CategoryLabel encode_html="1"$>"><$mt:CategoryLabel$></a></h3>
<mt:setvarblock name="cat"><$mt:CategoryLabel$></mt:setvarblock>
<mt:Entries category="$cat" include_subcategories="1" lastn="4">
<mt:if name="__first__">
            <ul>
</mt:if>
              <li><a href="<$mt:EntryPermalink$>" title="<$mt:EntryTitle encode_html="1"$>"><$mt:EntryTitle encode_html="1"$></a></li>
<mt:if name="__last__">
            </ul>
</mt:if>
</mt:Entries>
          </div>
</mt:TopLevelCategories>
        </div><!-- //end headlines -->

      </div><!-- //end .content -->
      </div><!-- //end #content -->
      <$mt:include module="Sidebar"$>
      <$mt:include module="Banner Footer"$>
    </div>
  </div>
</body>
</html>
