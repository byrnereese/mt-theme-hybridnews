<html xmlns="http://www.w3.org/1999/xhtml" dir="ltr" lang="en-US" xml:lang="en-US">
<head profile="http://gmpg.org/xfn/11">
    <title><$mt:BlogName$><mt:if tag="BlogDescription" ne=""> | <$mt:BlogDescription encode_html="1"$></mt:if></title>
    <meta name="robots" content="index,follow" />
    <meta name="description" content="<$mt:BlogDescription encode_html="1"$>" />
<mt:ignore>
    <meta name="copyright" content="Copyright (c) February 2009" />
    <meta name="revised" content="Monday, February 2nd, 2009, 2:32 pm" />
    <meta name="template" content="Hybrid 0.5" />
</mt:ignore>
    <link rel="EditURI" type="application/rsd+xml" title="RSD" href="<$MTLink template="rsd"$>" />
    <$mt:include module="HTML Head"$>
    <link rel="stylesheet" href="<$mt:StaticWebPath$>plugins/HybridNews/css/front-page.css?ver=0.1" type="text/css" media="screen" />
</head>
<body class="ltr not-logged-in home front-page page page-7 page-template page-template-front-page page-author-admin mac firefox firefox-3 primary-active secondary-active subsidiary-active">
  <div id="body-container">
    <$mt:include module="Banner Header"$>
    <div id="container">
      <div id="content"> <!-- Deprecated. Use the .content or .hfeed class for styling instead of #content. -->
      <div class="hfeed content">

        <mt:IfSlideshowEnabled>
        <$mt:include module="Homepage Slideshow"$>
        </mt:IfSlideshowEnabled>

<mt:setvarblock name="excerptcount"><$mt:ExcerptCount$></mt:setvarblock>
        <div id="excerpts">
<mt:Entries unique="1" lastn="$excerptcount">
<$mt:include module="Entry Summary"$>
</mt:Entries>
        </div><!-- //end excerpts -->

        <div id="headlines">
<mt:var name="count" value="0">
<mt:TopLevelCategories>
<mt:unless tag="CategoryExcludeFromHomepage">
<mt:setvar name="count" op="add" value="1">
<mt:setvarblock name="odd"><mt:var name="count" op="mod" value="2"></mt:setvarblock>

          <div class="section <mt:if name="odd">odd<mt:else>even</mt:if>">
            <h3 class="section-title"><a href="<$mt:CategoryArchiveLink$>" title="<$mt:CategoryLabel encode_html="1"$>"><$mt:CategoryLabel$></a></h3>
<mt:setvarblock name="cat"><$mt:CategoryLabel$></mt:setvarblock>
<mt:Entries category="$cat" include_subcategories="1" lastn="4">
<mt:if name="__first__">
            <ul>
</mt:if>
              <li><a href="<$mt:EntryPermalink$>" title="<$mt:EntryTitle encode_html="1"$>"><$mt:EntryTitle encode_html="1"$></a></li>
<mt:if name="__last__">
            </ul>
</mt:if>
</mt:Entries>
          </div>
</mt:unless>
</mt:TopLevelCategories>
        </div><!-- //end headlines -->

      </div><!-- //end .content -->
      </div><!-- //end #content -->

      <div id="widget-container">
        <div id="primary" class="aside">
          <mt:widgetset name="Homepage (primary)">
        </div>
        <mt:widgetset name="Homepage (secondary)">
      </div>

      <$mt:include module="Banner Footer"$>
    </div>
  </div>
</body>
</html>
